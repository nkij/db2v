{
  "expo": {
    "name": "dB Voltage Converter",
    "slug": "db-voltage-converter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f9fafb"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.antimatterwebcomics.dbvoltageconverter",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./icons/icon-512x512.png",
        "backgroundColor": "#f9fafb"
      },
      "package": "com.antimatterwebcomics.dbvoltageconverter"
    },
    "plugins": [
      "expo-build-properties"
    ],
    "extra": {
      "eas": {
        "projectId": "6e098ade-1496-462e-ba19-c246f2850985"
      }
    }
  }
}
